{
  "name": "mst-decorators",
  "version": "2.1.3",
  "license": "WTFPL",
  "description": "Class based MobX-State-Tree definitions",
  "repository": "farwayer/mst-decorators",
  "author": "farwayer <farwayer@gmail.com>",
  "scripts": {
    "prepack": "yarn clean && yarn cjs && yarn esm && yarn test",
    "cjs": "babel --out-dir cjs src",
    "esm": "ESM=1 babel --out-dir esm src",
    "clean": "rimraf cjs esm",
    "test": "jest",
    "cover": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "main": "cjs",
  "module": "esm",
  "react-native": "src",
  "types": "src/index.d.ts",
  "sideEffects": false,
  "dependencies": {
    "decorating": "^1.2.2",
    "istp": "^1.0.1",
    "rambda": "^5.1.1"
  },
  "peerDependencies": {
    "mobx-state-tree": ">=3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.1",
    "coveralls": "^3.1.0",
    "jest": "^25.4.0",
    "mobx": "^5.15.4",
    "mobx-state-tree": "^3.16.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "mobx",
    "mobx-state-tree",
    "decorators",
    "class"
  ],
  "files": [
    "src",
    "cjs",
    "esm"
  ]
}
